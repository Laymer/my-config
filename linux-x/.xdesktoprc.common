#!/bin/bash

if [ ! -e $HOME/.xsession-init-guard ]; then
    xcape -e 'Super_L=Super_L|F12'
    nitrogen --restore
    update-rofi-config
    fcitx
    compton --config $HOME/.config/compton/compton.conf &
    xscreensaver -no-splash &
    cbatticon -n &
    # wicd-gtk -t &
    nm-applet &
    # synapse -s &
    # plank &
fi

touch $HOME/.xsession-init-guard
